{
  "name": "@mirohq/miro-node",
  "version": "0.0.1-alpha.8",
  "description": "NodeJS client for Miro's REST API",
  "repository": {
    "type": "git",
    "url": "https://github.com/miroapp/api-clients.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf node_modules/ *.js",
    "build": "tsc",
    "build:docs": "typedoc && yarn move-docs-to-dist",
    "move-docs-to-dist": "cp -r 'docs-out' 'dist'",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "author": "Miro",
  "license": "MIT",
  "dependencies": {
    "generator": "*",
    "node-fetch": "^2.6.7",
    "typedoc": "^0.23.10"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^12",
    "@types/node-fetch": "^2.6.2",
    "jest": "^29.0.0",
    "typescript": "^4.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "format": "esm"
        }
      ]
    }
  }
}
