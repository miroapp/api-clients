{
  "name": "@mirohq/miro-node",
  "version": "0.0.1-alpha.8",
  "description": "NodeJS client for Miro's REST API",
  "repository": {
    "type": "git",
    "url": "https://github.com/miroapp/api-clients.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf node_modules/ *.js",
    "build": "tsc",
    "docs": "typedoc",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "author": "Miro",
  "license": "MIT",
  "dependencies": {
    "node-fetch": "^3.2.10",
    "typedoc": "^0.23.10"
  },
  "devDependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^12",
    "jest": "^28.1.3",
    "typescript": "^4.0"
  },
  "jest": {
    "extensionsToTreatAsEsm": [
      ".ts"
    ],
    "transform": {
      "^.+\\.tsx?$": [
        "esbuild-jest",
        {
          "format": "esm"
        }
      ]
    }
  }
}
