{
    "name": "{{npmName}}",
    "version": "{{npmVersion}}",
    "description": "NodeJS client for {{npmName}}",
    "repository": {
        "type": "git",
        "url": "https://{{gitHost}}/{{gitUserId}}/{{gitRepoId}}.git"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "clean": "rm -Rf node_modules/ *.js",
        "build": "tsc && typedoc api.ts index.ts",
        "test": "npm run build && node dist/client.js"
    },
    "author": "Miro",
    "license": "MIT",
    "dependencies": {
        "node-fetch": "^2.6.7",
        "typedoc": "^0.23.10"
    },
    "devDependencies": {
        "@types/node": "^12",
        "typescript": "^4.0",
        "@types/node-fetch": "^2.6.2"
    }{{#npmRepository}},
    "publishConfig": {
        "registry": "{{npmRepository}}"
    }{{/npmRepository}}
}
