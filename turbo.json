{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "generate": {
    },
    "build:docs": {
      "dependsOn": ["^build:docs"]
    },
    "build": {
      "dependsOn": ["generate", "^build"]
    },
    "dev": {
      "dependsOn": ["^dev"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": [],
      "inputs": ["__tests__/**/*.tsx"]
    },
    "lint": {
      "outputs": []
    },
    "deploy": {
      "dependsOn": ["build", "test", "lint"],
      "outputs": []
    }
  }
}