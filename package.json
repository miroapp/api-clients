{
  "name": "miro-api-clients",
  "private": true,
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "update-spec": "scripts/fetch-spec.mjs > packages/generator/spec.json",
    "build": "yarn workspace generator generate && yarn workspace @mirohq/miro-api build",
    "dev": "yarn workspace client-test-app dev",
    "pretest": "yarn build",
    "test": "yarn workspaces run test",
    "coverage": "yarn workspaces run coverage",
    "format": "prettier -w ./packages",
    "prepare": "husky install"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "prettier": "^2.8.7",
    "tsx": "^4.6.0"
  },
  "devDependencies": {
    "@mirohq/prettier-config": "^2.0.0",
    "@types/jest": "^29.5.8",
    "esbuild": "^0.19.8",
    "esbuild-jest": "^0.5.0",
    "husky": "^8.0.3"
  }
}
